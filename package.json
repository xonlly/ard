  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.23.0",
    "css-loader": "^0.26.2",
    "expect": "^1.20.2",
    "file-loader": "^0.10.0",
    "html-minify-loader": "^1.1.0",
    "mocha": "^3.2.0",
    "postcss-loader": "^1.3.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-unused": "^0.1.0"
  },
  "scripts": {
    "start": "webpack-dev-server --port 14246 --inline --devtool source-map --content-base ./ -d",
    "serve": "node -e \"require('babel-register');require('./script/devServer')\"",
    "build:server": "rm -rf build && babel --out-dir buildServer --ignore node_modules/**/*.js,script/*.js,lib/**/*.js server",
    "build:front": "rm -rf build && babel --out-dir buildFront --ignore node_modules/**/*.js,script/*.js,lib/**/*.js front",
    "build:watch:server": "yarn build:server -- --watch",
    "build:watch:front": "yarn build:front -- --watch",
    "start:server": "node buildServer/index.js",
    "start:front": "node buildFront/index.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "express": "^4.14.1",
    "material-ui": "^0.17.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-tap-event-plugin": "^2.0.1",
    "react-webcam": "^0.0.14",
    "redux": "^3.6.0",
    "refinery-js": "^0.6.0",
    "remote-redux-devtools": "^0.5.7",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "stats.js": "^0.17.0",
    "tracking": "^1.1.3",
    "whatwg-fetch": "^2.0.2"
  }
